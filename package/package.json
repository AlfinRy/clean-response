{
  "name": "@leviosary/clean-response",
  "version": "1.0.0",
  "description": "A lightweight, type-safe TypeScript package for standardizing API responses in backend applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "api",
    "response",
    "express",
    "fastify",
    "typescript",
    "backend",
    "rest",
    "api-response",
    "http-response",
    "nodejs",
    "type-safe",
    "lightweight",
    "clean",
    "standard"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "tsup": "^8.5.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
